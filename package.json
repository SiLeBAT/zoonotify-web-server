{
  "name": "@SiLeBAT/zoonotify-web-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/SiLeBAT/zoonotify-web-server.git"
  },
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "start": "node ./lib/bundle.js",
    "serve": "npx nodemon -e ts --exec \"npm run serve:pipe\"",
    "debug": "npx nodemon -e ts --exec \"npm run debug:pipe\"",
    "serve:pipe": "npm run build && node ./lib/bundle.js",
    "debug:pipe": "npm run build && node --inspect-brk ./lib/bundle.js",
    "cp:json": "npx copyfiles -u 1 ./src/**/*.json ./dist",
    "cp:all": "npm run cp:json",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run compile && npm run cp:all && ./node_modules/.bin/webpack",
    "prebuild": "npm run clean",
    "clean": "npx rimraf lib/* && npx rimraf dist/*",
    "lint": "npx eslint ./src --ext .js,.ts",
    "lint:fix": "npx eslint --fix ./src --ext .js,.ts",
    "format": "npx prettier --write \"src/**/*.ts\""
  },
  "resolutions": {
    "tar": "4.4.19"
  },
  "private": false,
  "dependencies": {
    "@SiLeBAT/fg43-ne-server": "^1.0.3",
    "app-root-dir": "^1.0.2",
    "compression": "^1.7.3",
    "config": "^1.29.4",
    "core-js": "^2.6.12",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "helmet": "^3.23.3",
    "inversify": "^5.1.1",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^8.1.0",
    "linebyline": "^1.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "pg": "^8.7.1",
    "pm2": "~5.2.0",
    "processenv": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "request-promise-native": "^1.0.9",
    "rxjs": "^6.6.7",
    "sequelize": "^6.6.5",
    "swagger-ui-express": "^4.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/app-root-dir": "^0.1.0",
    "@types/compression": "0.0.36",
    "@types/config": "0.0.34",
