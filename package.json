  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "env": {
      "node": true
    },
    "ignorePatterns": [
      "**/__mocks__/*.ts",
      "**/__tests__/*.ts"
    ],
    "rules": {}
  },
